         function getDependentMember() {
             let dependentMember = [];
         
             for(var i=0;i<memberId.length;i++) {
                 var fn = document.getElementById("fn" + memberId[i]).getElementsByTagName("input")[0].value.trim();
                 var ln = document.getElementById("ln" + memberId[i]).getElementsByTagName("input")[0].value.trim();
                 var fn_and_ln = document.getElementById("fn_and_ln" + memberId[i]).getElementsByTagName("input")[0].value.trim();

                 if(fn!=="" && ln!=="") {
                     var fullName = fn + " " + ln;

                     if(fullName.replaceAll(" ", "")!="") {                
                           dependentMember = [...dependentMember, fullName.trim()];
                     }
                  } else if(fn_and_ln!=="") {
                     var fullName = fn_and_ln;
                     if(fullName.replaceAll(" ", "")!="") {                
                           dependentMember = [...dependentMember, fullName.trim()];
