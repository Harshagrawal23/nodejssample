         function getShippingAddress() {
            var shippingAddress = "";
            shippingAddress = appendString(shippingAddress, address1);
            shippingAddress = appendString(shippingAddress, address2);
            shippingAddress = appendString(shippingAddress, city);
            shippingAddress = appendString(shippingAddress, state);
            shippingAddress = appendString(shippingAddress, zipcd, "");
            return shippingAddress.trim();
         }

         function appendString(str1, str2, delimeter = ",") {
            if(str1==null && str2==null)
               return "";
            else if(str1==null || str1=="")
               return str2;
            else if(str2==null || str2=="")
               return str1;
            else if(str1!=="" && str2!=="")
               return str1 + `${delimeter} ` + str2;
            else
               return "";
         }
         
