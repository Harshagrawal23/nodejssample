      <script>
         async function fetchEstimate() {
             membersCount = getMembersCount();
             let total = await fetchEstimateForNewSubscription(itemPriceId,  membersCount, email, address1, city, zipcd, "US");
             total = total*1.0/100;
             const estimateText = document.getElementById("estimate");
             estimateText.textContent = `$${total}/${periodUnitAbbr}`;
             document.getElementById("estimate1").textContent =  `$${total}/${periodUnitAbbr}`;
             const estimateSupComp = document.getElementsByClassName("estimate-amount-sup");

             for(var i=0;i<estimateSupComp.length;i++) {
                estimateSupComp[i].textContent =  `${total}/${periodUnitAbbr}`;
             }
         }
         
         async function updateCheckout(display=false) {
             membersCount = getMembersCount();
             if(email!=undefined && email!="") {
               createCheckoutForNewSubscription(itemPriceId, membersCount, fname, lname , email, phone , address1, address2, city, zipcd, state, "US", billingAddressSameAsMailing? address1 : baddress1 , billingAddressSameAsMailing? address2 : baddress2, billingAddressSameAsMailing? city : bcity, billingAddressSameAsMailing? zipcd : bzipcd, billingAddressSameAsMailing? state : bstate, getDependentMember(), isr )
               .then(hosted_page => {
                  document.getElementById("checkoutIframe").src = hosted_page.url;
                  document.getElementById("checkoutIframe").style.display = "none";

                  var delayInMilliseconds = 10000; //1 second

                  setTimeout(function() {
                     document.getElementById("checkoutIframe").style.display = "block";
                  }, delayInMilliseconds);


               }) .catch( err => {
                  console.log(err);
               } )
            }
         }
         
         async function openCheckoutPopup() {
             if(isCheckoutPossible()) {
                 membersCount = getMembersCount();
                 createCheckoutPopup(siteName, itemPriceId, membersCount, fname, lname , email, phone , address1, address2, city, zipcd, state, "US", billingAddressSameAsMailing? address1 : baddress1 , billingAddressSameAsMailing? address2 : baddress2, billingAddressSameAsMailing? city : bcity, billingAddressSameAsMailing? zipcd : bzipcd, billingAddressSameAsMailing? state : bstate, getDependentMember(), isr )
                 .then(hosted_page => {
                 }) .catch( err => {
                     console.log(err);
                 } )
             }
         }
         
         
         function isCheckoutPossible() {  
            var button = document.getElementById("checkout-button");
            var flag = itemPriceId && fname && lname && email && phone && address1 && city && zipcd && state && ( billingAddressSameAsMailing || (baddress1 && bcity && bzipcd && bstate) ) && (counter===getMembersCount()-1);

            if(flag) {

               flag = validateName(fname) && validateName(lname) && validateEmail(email) && validatePhone(phone) && validateCity(city) && validateZipCode(zipcd);
 
               if(!billingAddressSameAsMailing) {
                  flag = flag && validateCity(bcity) && validateZipCode(zipcd);
               }

               flag = flag && validateDependentMember();
            }

            button.disabled = !flag;
         
            button.style.backgroundColor = flag? "#6FB231" : "#A3A3A3";
            return flag;
         }
         
         function checkForFullDependentName() {
         
            for(var i=0;i<memberId.length;i++) {
         
               var fn = document.getElementById("fn" + index[i]).getElementsByTagName("input")[0].value.trim();
               var ln = document.getElementById("ln" + index[i]).getElementsByTagName("input")[0].value.trim();
            
               if(fn==="" || ln==="") {
                  return false;
               }
            }
         
             return true;
         }
         
         async function displayCheckout() {
             showCheckout = true;
             updateCheckout(true);
         }
         
         
         function findPeriodUnit() {
             periodUnit = itemPrice.item_price.period_unit;
             if(periodUnit==="month")
                 periodUnitAbbr = "mo";
             else if(periodUnit==="year")
                 periodUnitAbbr = "yr";
             if(itemPrice.item_price.period>1)
                 periodUnitAbbr = itemPrice.item_price.period + periodUnitAbbr;
         }
         
         async function onPageLoad() {
             //item = await fetchItem(itemId);
             itemPrice = await fetchItemPrice(itemPriceId); 
             findPeriodUnit();
             fetchEstimate();
             initializeStateDropDown();
             var planNameComp = document.getElementsByClassName("plan-name");
             for(var i=0;i<planNameComp.length;i++)
                 planNameComp[i].textContent = itemPrice.item_price.external_name;
             
             updatePrimaryMemberPrice();
             fetchItem(itemId).then(res => {
                item = res;
                createCheckoutSummary();
             })
             calculateAdditionalAmountForNewMember();
             updateAdditionalPrice(1);
         }

         async function initializeStateDropDown() {
            let states = await fetchAllState();
            states = states.state;

            let billingStateComp = document.getElementById("Billingstate");
            let stateComp = document.getElementById("state");

            for(var i=0;i<states.length;i++) {

               let optionComp = document.createElement("option");
               optionComp.textContent = states[i].value;
               optionComp.value = states[i].value;

               let optionComp1 = document.createElement("option");
               optionComp1.textContent = states[i].value;
               optionComp1.value = states[i].value;

               stateComp.appendChild(optionComp);
               billingStateComp.appendChild(optionComp1);

            }
            
         }
         
         window.onload = onPageLoad;
      </script>
