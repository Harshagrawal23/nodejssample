         function validateFields(fieldName, fieldComponent, value, validateFieldValue=false ,validator, renderError){
            removeValidationMessage(fieldComponent);
             
            if(fieldComponent.validity.rangeOverflow && fieldComponent.parentElement.children.length == 1 && (value == undefined || value == "")){
               showRangeErrorValidationMessage(fieldComponent);
               return;
            }else if(fieldComponent.validity.valueMissing && fieldComponent.parentElement.children.length == 1 && (value == undefined || value == "")){
                 showMissingErrorValidationMessage(fieldComponent);
                 return;
            }

            if(validateFieldValue) {
               if(!validator(value)) {
                  renderError(fieldComponent, fieldName);
               }
            }
         }

         function showRangeErrorValidationMessage(fieldComponent) {
            var divElement = document.createElement("div");
            divElement.id = "fnrangeerror";
            divElement.className = "required-error-";

            var spanElement = document.createElement("span");
            spanElement.className = "required-error";
            spanElement.textContent =`The ${fieldName} is too large`;
            divElement.appendChild(spanElement);

            fieldComponent.parentElement.appendChild(divElement);
         }

         function showMissingErrorValidationMessage(fieldComponent) {
            var divElement = document.createElement("div");
            divElement.id = "missingerror";
            divElement.className = "required-error-";

            var spanElement = document.createElement("span");
            spanElement.className = "required-error";
            spanElement.textContent = "Please enter required information";
            divElement.appendChild(spanElement);

            fieldComponent.parentElement.appendChild(divElement);
         }

         
         function showValidationMessage(fieldComponent){
            removeValidationMessage(fieldComponent);
             if(fieldComponent.parentElement.children.length == 1){
              /*  var ele = document.createElement("div");
                ele.className = "required-error-";
                ele.innerHTML = '<span class="required-error">Cannot contain special characters.</span>';
                ele.id = 'valerror';*/
               var divElement = document.createElement("div");
               divElement.id = "valerror";
               divElement.className = "required-error-";

               var spanElement = document.createElement("span");
               spanElement.className = "required-error";
               spanElement.textContent = "Cannot contain special characters.";
               divElement.appendChild(spanElement);
               fieldComponent.parentElement.appendChild(divElement);
             }

         }

         function showInvalidFieldValueMessage(fieldComponent, fieldName) {
            removeValidationMessage(fieldComponent);
             if(fieldComponent.parentElement.children.length == 1){
                /*var ele = document.createElement("div");
                ele.className = "required-error-";
                ele.innerHTML = '<span class="required-error">Must be a valid ' + fieldName + '</span>';
                ele.id = 'valerror';*/
               var divElement = document.createElement("div");
               divElement.id = "valerror";
               divElement.className = "required-error-";

               var spanElement = document.createElement("span");
               spanElement.className = "required-error";
               spanElement.textContent = "Must be a valid " + fieldName;
               divElement.appendChild(spanElement);
               fieldComponent.parentElement.appendChild(divElement);
             }            
         }
         
         function removeValidationMessage(fieldComponent){
             if(fieldComponent.parentElement.children.length > 1){
                 var errdiv = fieldComponent.parentElement.lastElementChild;
                 if(errdiv.id == 'valerror' || errdiv.id == 'missingerror'){
                     fieldComponent.parentElement.removeChild(fieldComponent.parentElement.lastElementChild);
                 }
             }
         }

         function validateEmail(email){
            return String(email)
               .toLowerCase()
               .match(
                  /^(([^<>()[\]\\.,;:\s@"]+(\.[^<>()[\]\\.,;:\s@"]+)*)|(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/
               );
         };

         function validateName(name) {
            var pattern = /^([a-zA-Z`'~\.\-]+)+$/;
            return pattern.test(name);
         }

         function validateFullName(name) {
            var pattern = /^([a-zA-Z`'~\.\-]+)+ ?([a-zA-Z`'~\.\-]+)+$/;
            return pattern.test(name);
         }

         function validatePhone(phone) {
          //  var pattern = /^([0-9-() ]){10}$/;
            var pattern = /^\s*(?:\+?(\d{1,3}))?[-. (]*(\d{3})[-. )]*(\d{3})[-. ]*(\d{4})(?: *x(\d+))?\s*$/;
            return pattern.test(phone);
         }

         function validateCity(city) {
            var pattern = /^([a-zA-z\-\.' ])+$/;
            return pattern.test(city);            
         }

         function validateZipCode(zipCode) {
            var pattern = /^([0-9]){5}$/;
            return pattern.test(zipCode);
         }

         
         
