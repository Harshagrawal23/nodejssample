         function calculateAdditionalAmountForNewMember() {
             let tiers = itemPrice.item_price.tiers;
             let current_price;
             let next_price;
             let last_tier = 0;
         
             for(var qty = 2; qty <=limit+1; qty++) { 
                 for(var i=last_tier;i<tiers.length;i++) {
                     let su = tiers[i].starting_unit;
                     let eu = tiers[i].ending_unit;
                     if(eu==null)
                         eu = Number.MAX_SAFE_INTEGER;
         
                     if(qty-1>=su && qty-1<=eu)
                         current_price = tiers[i].price;
         
                     if(qty>=su && qty<=eu)
                         next_price = tiers[i].price;
         
                     last_tier = i;
                 }
                 additionalPrice[qty-2] = (next_price - current_price)*1.0/100;
                 current_price = next_price;
             }
         }
         
         function updateAdditionalPrice(additional_qty) {
             let additional_price = additional_qty<limit ? additionalPrice[additional_qty-1] : 0;
             let buttonElement = document.getElementById("id_additional_amount");
         
             if(additional_price===0)
                 buttonElement.textContent = "Add More Members for FREE";
             else if(memberId.length === 0)
                 buttonElement.textContent = `Add Member ($${additional_price})`;
             else
                 buttonElement.textContent = `Add Another Member ($${additional_price})`;
         }
         
